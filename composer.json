{
    "name": "deesoft/yii2-application",
    "description": "Yii2 application template",
    "keywords": ["yii2", "Application"],
    "homepage": "http://www.sangkilsoft.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Misbahul Munir",
            "email": "misbahuldmunir@gmail.com",
            "role": "Core project development"
        }
    ],
    "support": {
        "issues": "https://github.com/deesoft/yii2-app-single/issues?state=open",
        "source": "https://github.com/deesoft/yii2-app-single"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-jui": "~2.0",
        "yiisoft/yii2-bootstrap": "~2.0",
        "mdmsoft/yii2-admin": "~2.0",
        "mdmsoft/yii2-autonumber": "~1.0",
        "mdmsoft/yii2-format-converter": "~1.0",
        "mdmsoft/yii2-ar-behaviors": "~1.0",
        "deesoft/yii2-console": "~1.0",
        "deesoft/yii2-angular": "~1.0",
        "deesoft/yii2-rest": "~1.0",
        "deesoft/yii2-rest-base": "~1.0",
        "deesoft/yii2-base": "~1.0",
        "deesoft/biz3-client": "~3.0",
        "deesoft/biz3-api": "~3.0",
        "deesoft/yii2-adminlte": "~1.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0",
        "yiisoft/yii2-debug": "~2.0",
        "deesoft/yii2-gii": "~1.0",
        "yiisoft/yii2-faker": "~2.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
